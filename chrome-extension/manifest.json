
{
  "manifest_version": 3,
  "name": "USTA S",
  "version": "1.0",
  "description": "A Chrome extension to parse and display tennis match data.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "http://localhost:8080/",
    "https://tennislink.usta.com/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://tennislink.usta.com/Leagues/reports/NTRP/*"],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
